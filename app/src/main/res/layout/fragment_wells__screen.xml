<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f7f7f7"
    tools:context=".Wells_Screen">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="843dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:id="@+id/wells_banner"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:paddingLeft="15dp"
            android:paddingTop="15dp"
            android:paddingBottom="15dp"
            app:layout_constraintBaseline_toTopOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/wells_title"
                android:layout_width="200dp"
                android:layout_height="wrap_content"
                android:layout_gravity="start"
                android:fontFamily="@font/roboto_bold"
                android:paddingLeft="10dp"
                android:text="@string/Wells_title"
                android:textColor="#363B3C"
                android:textSize="25sp"
                app:layout_constraintHorizontal_bias="0.498" />


        </LinearLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="600dp"
            android:layout_marginTop="16dp"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@id/wells_banner">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:paddingBottom="40dp">

                <View
                    android:id="@+id/scroclltop"
                    android:layout_width="340dp"
                    android:layout_height="1dp"
                    android:background="#D8D8D8"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <CheckBox
                    android:id="@+id/question1"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginRight="30dp"
                    android:fontFamily="@font/roboto"
                    android:layoutDirection="rtl"
                    android:paddingLeft="30dp"
                    android:paddingTop="5dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="5dp"
                    android:text="@string/question_1"
                    android:textColor="#363B3C"
                    android:textSize="15sp"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <View
                    android:layout_width="340dp"
                    android:layout_height="1dp"
                    android:background="#D8D8D8"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question1" />

                <CheckBox
                    android:id="@+id/question2"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginRight="30dp"
                    android:fontFamily="@font/roboto"
                    android:layoutDirection="rtl"
                    android:paddingLeft="30dp"
                    android:paddingTop="5dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="5dp"
                    android:text="@string/question_2"
                    android:textColor="#363B3C"
                    android:textSize="15sp"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question1" />

                <View
                    android:layout_width="340dp"
                    android:layout_height="1dp"
                    android:background="#D8D8D8"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question2" />

                <CheckBox
                    android:id="@+id/question3"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginRight="30dp"
                    android:fontFamily="@font/roboto"
                    android:layoutDirection="rtl"
                    android:paddingLeft="30dp"
                    android:paddingTop="5dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="5dp"
                    android:text="@string/question_3"
                    android:textColor="#363B3C"
                    android:textSize="15sp"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question2" />

                <View
                    android:layout_width="340dp"
                    android:layout_height="1dp"
                    android:background="#D8D8D8"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question3" />

                <CheckBox
                    android:id="@+id/question4"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginRight="30dp"
                    android:fontFamily="@font/roboto"
                    android:layoutDirection="rtl"
                    android:paddingLeft="30dp"
                    android:paddingTop="5dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="5dp"
                    android:text="@string/question_4"
                    android:textColor="#363B3C"
                    android:textSize="15sp"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question3" />

                <View
                    android:layout_width="340dp"
                    android:layout_height="1dp"
                    android:background="#D8D8D8"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question4" />

                <CheckBox
                    android:id="@+id/question5"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginRight="30dp"
                    android:fontFamily="@font/roboto"
                    android:layoutDirection="rtl"
                    android:paddingLeft="30dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:text="@string/question_5"
                    android:textColor="#363B3C"
                    android:textSize="15sp"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question4" />

                <View
                    android:layout_width="340dp"
                    android:layout_height="1dp"
                    android:background="#D8D8D8"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question5"
                    tools:layout_editor_absoluteX="40dp" />

                <CheckBox
                    android:id="@+id/question6"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginRight="30dp"
                    android:fontFamily="@font/roboto"
                    android:layoutDirection="rtl"
                    android:paddingLeft="30dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:text="@string/question_6"
                    android:textColor="#363B3C"
                    android:textSize="15sp"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question5" />

                <View
                    android:layout_width="340dp"
                    android:layout_height="1dp"
                    android:background="#D8D8D8"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question6" />

                <CheckBox
                    android:id="@+id/question7"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginRight="30dp"
                    android:fontFamily="@font/roboto"
                    android:layoutDirection="rtl"
                    android:paddingLeft="30dp"
                    android:paddingTop="5dp"
