                    android:textColor="#363B3C"
                    android:textSize="15sp"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question6" />

                <View
                    android:layout_width="340dp"
                    android:layout_height="1dp"
                    android:background="#D8D8D8"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question7" />

                <CheckBox
                    android:id="@+id/question8"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginRight="30dp"
                    android:fontFamily="@font/roboto"
                    android:layoutDirection="rtl"
                    android:paddingLeft="30dp"
                    android:paddingTop="5dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="5dp"
                    android:text="@string/question_8"
                    android:textColor="#363B3C"
                    android:textSize="15sp"
                    app:layout_constraintHorizontal_bias="0.0"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question7" />

                <View
                    android:layout_width="340dp"
                    android:layout_height="1dp"
                    android:background="#D8D8D8"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question8" />

                <CheckBox
                    android:id="@+id/question9"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginRight="30dp"
                    android:fontFamily="@font/roboto"
                    android:layoutDirection="rtl"
                    android:paddingLeft="30dp"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp"
                    android:text="@string/question_9"
                    android:textColor="#363B3C"
                    android:textSize="15sp"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question8" />

                <View
                    android:layout_width="340dp"
                    android:layout_height="1dp"
                    android:background="#D8D8D8"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question9" />

                <CheckBox
                    android:id="@+id/question10"
                    android:layout_width="match_parent"
                    android:layout_height="65dp"
                    android:layout_marginRight="30dp"
                    android:fontFamily="@font/roboto"
                    android:layoutDirection="rtl"
                    android:paddingLeft="30dp"
                    android:paddingTop="5dp"
                    android:paddingRight="20dp"
                    android:paddingBottom="5dp"
                    android:text="@string/question_10"
                    android:textColor="#363B3C"
                    android:textSize="15sp"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question9" />

                <View
                    android:layout_width="340dp"
                    android:layout_height="1dp"
                    android:background="#D8D8D8"
                    app:layout_constraintHorizontal_bias="0.501"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question10" />


                <androidx.appcompat.widget.AppCompatButton
                    android:id="@+id/wells_submit"
                    android:layout_width="150dp"
                    android:layout_height="41dp"
                    android:layout_gravity="center"
                    android:layout_marginTop="30dp"
                    android:alpha="0.85"
                    android:background="@drawable/button"
                    android:backgroundTint="#82CB70"
                    android:drawableStart="@drawable/ic_baseline_file_upload_24"
                    android:drawablePadding="0dp"
                    android:elevation="0dp"
                    android:fontFamily="@font/roboto_bold"
                    android:gravity="center"
                    android:letterSpacing=".05"
                    android:paddingHorizontal="30dp"
                    android:text="@string/submit"
                    android:textAllCaps="false"
                    android:textColor="#F7F8F9"
                    android:textSize="16sp"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/question10" />
            </androidx.constraintlayout.widget.ConstraintLayout>
        </ScrollView>

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>